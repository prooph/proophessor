{
    "name": "prooph/proophessor",
    "description": "CQRS plus ES for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "prooph",
        "CQRS",
        "Event Sourcing",
        "ZF2"
    ],
    "homepage": "https://github.com/prooph/proophessor",
    "require": {
        "php": ">=5.5",
        "zendframework/zend-servicemanager": "~2.4",
        "prooph/service-bus": "~3.1",
        "prooph/event-sourcing": "~2.1",
        "prooph/event-store": "~3.1",
        "prooph/event-store-doctrine-adapter": "~1.0",
        "codeliner/array-reader" : "~1.1"
    },
    "require-dev" : {
        "phpspec/phpspec": "~2.0",
        "bossa/phpspec2-expect" : "~1.0",
        "henrikbjorn/phpspec-code-coverage": "~1.0",
        "satooshi/php-coveralls": "dev-master",
        "prooph/event-store-zf2-adapter": "~1.0",
        "zendframework/zend-db": "~2.4"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\Proophessor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prooph\\Proophessor\\Stub\\": "stubs/"
        }
    }
}
